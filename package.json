		"async"
	],
	"dependencies": {
		"pify": "^2.3.0"
	},
	"devDependencies": {
		"@shinnn/eslint-config": "^6.7.0",
		"eslint": "^5.5.0",
		"nyc": "^13.0.1",
		"nyc-config-common": "^1.0.0",
		"tape": "^4.9.1"
	},
	"eslintConfig": {
		"extends": "@shinnn",
		"rules": {
			"promise/prefer-await-to-then": "off"
		}
	},
	"nyc": {
		"extends": "nyc-config-common"
	}
}
