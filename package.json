{
	"name": "rmf",
	"version": "2.0.0-3",
	"description": "Remove a file or symbolic link without throwing an error when the target does not exist",
	"repository": "shinnn/rmf",
	"author": "Shinnosuke Watanabe (https://github.com/shinnn)",
	"license": "ISC",
	"scripts": {
		"pretest": "eslint .",
		"test": "nyc node test.js"
	},
	"files": [
